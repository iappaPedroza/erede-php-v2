{
  "name": "iappapedroza/erede-php-v2",
  "version": "2.0.0",
  "description": "PHP SDK para integração com a API eRede v2 (OAuth 2.0)",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "psr/log": "*",
    "monolog/monolog": "^3.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "^7.0 — Required only when using GuzzleOAuthService as an alternative HTTP client for OAuth requests."
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.5 || ^11.0",
    "friendsofphp/php-cs-fixer": "^v3.11.0",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7",
    "guzzlehttp/guzzle": "^7.10"
  },
  "autoload": {
    "psr-4": {
      "Rede\\": "src/Rede"
    }
  },
  "authors": [
    {
      "name": "João Batista Neto",
      "email": "neto.joaobatista@gmail.com"
    },
    {
      "name": "Iaponã Pedroza de Andrade",
      "email": "contato@itstecnologiacriativa.com.br"
    }
  ]
}
